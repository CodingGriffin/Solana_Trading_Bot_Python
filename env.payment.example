# Bot Configuration
BOT_TOKEN=your_telegram_bot_token_here
BOT_USERNAME=YourBotUsername

# Database Configuration
DATABASE_URL=mongodb://localhost:27017/solana_bot

# Solana Configuration
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_WS_URL=wss://api.mainnet-beta.solana.com
PRIVATE_KEY=your_base58_encoded_private_key_here

# Trading Configuration
DEFAULT_SLIPPAGE=0.5
MAX_TRADE_AMOUNT=10.0
MIN_TRADE_AMOUNT=0.01

# Whale Detection Thresholds
WHALE_THRESHOLD_SOL=1000
WHALE_THRESHOLD_USD=100000

# Monitoring Configuration
MONITOR_INTERVAL=5
MAX_CONCURRENT_MONITORS=100

# DEX Configuration
RAYDIUM_PROGRAM_ID=675kPX9MHTjS2zt1qfr1NYHuzeLXfQM9H24wFSUt1Mp8
SERUM_PROGRAM_ID=9xQeWvG816bUx9EPjHmaT23yvVM2ZWbrrpZb9PusVFin

# Alert Configuration
TELEGRAM_ALERTS=true
DISCORD_ALERTS=false

# Performance Configuration
ASYNC_WORKERS=10
CACHE_TTL=300

# Logging Configuration
LOG_LEVEL=WARNING
BACKUP_ENABLED=true
SYSTEM_ALERTS_ENABLED=true
LOG_FILE=solana_bot.log

# Admin Configuration
ADMIN_CHAT_ID=your_admin_chat_id_here

# Admin Wallet Configuration (CRITICAL - Change these!)
ADMIN_WALLET_ADDRESS=your_admin_wallet_address_here
ADMIN_WALLET_PRIVATE_KEY=your_admin_wallet_private_key_here

# Payment Configuration
PAYMENT_GATEWAY_ENABLED=true
PAYMENT_CONFIRMATION_BLOCKS=3
MIN_PAYMENT_AMOUNT=0.001
SUBSCRIPTION_FEE_PERCENTAGE=0.1

# Fee Configuration
WALLET_CREATION_FEE=0.01
TRANSACTION_FEE_PERCENTAGE=0.5
SUBSCRIPTION_FEE_PERCENTAGE=0.1

# Subscription Management
SUBSCRIPTION_GRACE_PERIOD_DAYS=3
AUTO_SUBSCRIPTION_RENEWAL=true

# Wallet Management
WALLET_ENCRYPTION_KEY=your_encryption_key_here
MAX_USER_WALLETS=5
ENABLE_WALLET_CREATION=true
ENABLE_WALLET_IMPORT=true

# Security Configuration
MAX_REQUESTS_PER_MINUTE=60
RATE_LIMIT_WINDOW=60 
# Testnet Environment Configuration
# Copy this file to .env.testnet and configure your testnet settings

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Solana Testnet Configuration
SOLANA_RPC_URL=https://api.testnet.solana.com
SOLANA_WS_URL=wss://api.testnet.solana.com

# Testnet Database Configuration
TESTNET_DATABASE_URL=mongodb://localhost:27017/solana_bot_testnet

# Testnet Admin Configuration
TESTNET_ADMIN_CHAT_ID=your_admin_chat_id_here
TESTNET_ADMIN_WALLET_ADDRESS=your_testnet_admin_wallet_address_here

# Testnet Payment Configuration
TESTNET_ADMIN_WALLET_PRIVATE_KEY=your_testnet_admin_private_key_here

# Testnet Fee Configuration (Lower fees for testing)
TRANSACTION_FEE_PERCENTAGE=0.05
WALLET_CREATION_FEE=0.001
MIN_PAYMENT_AMOUNT=0.0001

# Testnet Trading Limits (Lower limits for testing)
MAX_TRADE_AMOUNT=1.0
MIN_TRADE_AMOUNT=0.001

# Testnet Whale Thresholds (Lower thresholds for testing)
WHALE_THRESHOLD_SOL=10
WHALE_THRESHOLD_USD=1000

# Testnet Network Configuration
PAYMENT_CONFIRMATION_BLOCKS=1

# Testnet Security (Relaxed for testing)
TESTNET_MAX_REQUESTS_PER_MINUTE=120
TESTNET_RATE_LIMIT_WINDOW=60

# Testnet Monitoring
TESTNET_MONITOR_INTERVAL=10
TESTNET_MAX_CONCURRENT_MONITORS=50

# Testnet Performance
TESTNET_ASYNC_WORKERS=5
TESTNET_CACHE_TTL=60

# Testnet Timeouts (Longer for testing)
TESTNET_TELEGRAM_TIMEOUT=120.0
TESTNET_TELEGRAM_CONNECT_TIMEOUT=30.0
TESTNET_TELEGRAM_READ_TIMEOUT=120.0
TESTNET_CALLBACK_TIMEOUT=20.0

# Testnet Logging
TESTNET_LOG_FILE=solana_bot_testnet.log
TESTNET_LOG_LEVEL=DEBUG

# Testnet Alert Configuration
TESTNET_ALERTS=true
TESTNET_ALERT_PREFIX=[TESTNET]

# Jupiter API (for price data)
JUPITER_API_URL=https://quote-api.jup.ag/v6

# Wallet Encryption (use a strong key for testnet)
WALLET_ENCRYPTION_KEY=your_testnet_encryption_key_here

# Testnet Subscription Tiers (Lower fees for testing)
# These are automatically configured in testnet_settings.py
# Premium: 0.01 SOL/month
# Pro: 0.05 SOL/month

# Testnet Features
ENABLE_WALLET_CREATION=true
ENABLE_WALLET_IMPORT=true
ENABLE_TRADING_WALLET_CONNECTION=true
PAYMENT_GATEWAY_ENABLED=true
AUTO_SUBSCRIPTION_RENEWAL=true

# Testnet Alert Settings
TELEGRAM_ALERTS=true
DISCORD_ALERTS=false
DISCORD_WEBHOOK_URL=

# Testnet Performance Settings
ASYNC_WORKERS=5
CACHE_TTL=60

# Testnet Network Settings
TELEGRAM_TIMEOUT=120.0
TELEGRAM_CONNECT_TIMEOUT=30.0
TELEGRAM_READ_TIMEOUT=120.0
CALLBACK_TIMEOUT=20.0

# Testnet Security Settings
MAX_REQUESTS_PER_MINUTE=120
RATE_LIMIT_WINDOW=60

# Testnet Logging Settings
LOG_LEVEL=DEBUG
LOG_FILE=solana_bot_testnet.log 